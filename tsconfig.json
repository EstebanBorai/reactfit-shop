{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "client/components/*",
      ],
      "reducers/*": [
        "client/reducers/*",
      ],
      "theme/*": [
        "client/theme/*",
      ],
      "types/*": [
        "client/types/*"
      ]
    }
  },
  "include": [
    "./client/**/*"
  ]
}
