.main-aside
  width: 50px
  background: #dedede
  height: 100vh

  .icon-container-list
    display: flex
    margin: 0
    padding: 0
    list-style-type: none
    height: 100%
    
    .icon-container
      width: 50px
      height: 50px
      cursor: pointer
      display: flex
      justify-content: center
      align-items: center

      &:hover
        transition: .4s
        background: #ccfff8

      
