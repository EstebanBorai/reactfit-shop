.lobby-nav
  grid-column: 1 / 1
  background: #373737
  color: #ffffff
  text-align: center
  padding: .5rem 0
  box-sizing: border-box
  height: 100vh

  header
    height: 60px
    display: inline-block
  
  ul
    padding: 0
    margin: 0
    list-style-type: none

    li
      height: 60px
      display: inline-block

